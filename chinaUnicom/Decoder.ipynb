    " from sklearn import preprocessing\n",
    "le = preprocessing.LabelEncoder()\n",
    "le.fit([1, 2, 2, 6])\n",
    "print(le.classes_)\n",
    "n = le.transform([1, 1, 2, 6]) # Transform Categories Into Integers\n",
    "print(n)\n",
